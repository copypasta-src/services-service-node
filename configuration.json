{
   
    "name" : "Microservices API",
    "description" : "API that handles all logic for the creation and management of microservices",
    "version" : "1.0.0",
    "type" : "microservice",
    "author" : "Zach Tucker",
    "gitUrl" : "https://github.com/opera-src/services-service-node",
    "ingredients" : {
        "cicd" : {
            "Github Actions" : {
                "cicdProvider" : "githubActions"
            }
        },
        "containerization" : {
            "Docker" : {
                "containerizationProvider" : "docker"
            }
        },
        "deployment" : {
            "AWS AppRunner + ECR" : {
                "deploymentProvider" : "aws",
                "deploymentProviderConfiguration" : {
                    "deploymentModule" : "AppRunner",
                    "deploymentContainerRepositorty" : "ECR"
                }
            }
        },
        "framework" : {
            "ExpressJS" : {
                "frameworkProvider" : "express"
            }
        },
        "git" : {
            "Github" : {
                "gitProvider" : "github"
            }
        }
    }
}